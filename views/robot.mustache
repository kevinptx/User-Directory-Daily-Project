<html>
<head>
  <link rel="stylesheet" href="/css/master.css">
</head>
<body>
  <h1></h1>
  <div class="robotDiv">
    <div class="robotAvatar">
      <img class="avatars" src="{{robot.avatar}}" alt="">
    </div>
    <div class="robotInfo">
      <h1 class="name">{{robot.name}}</h1>
      <h4 class="address">{{robot.address.city}}, {{robot.address.country}} | {{robot.job}},
      {{robot.company}}</h4>
      <h4 class="contact">{{robot.email}} | {{robot.phone}}</h4>

<div class="educationSkills">
  <div class="education">
    <div class="table-header">
      <h4 id="educationHeader">Education</h4>
    </div>
    <div class="university">
      <p id="university">{{robot.university}}</p>
    </div>
  </div>

  <div class="skills">
    <div class="table-header">
      <h4 id="skillsHeader">Skills</h4>
    </div>
    <div class="skillList">
      {{#robot.skills}}
      <p id="skillsList">{{.}}</p>
      {{/robot.skills}}
    </div>
  </div>

</div>
    </div>
  </div>

</body>
</html>
